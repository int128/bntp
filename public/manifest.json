{
  "name": "BNTP: Bookmarks in New Tab Page",
  "version": "0.0.0.0",
  "description": "Lightweight New Tab Page with Bookmarks and Top Sites",
  "permissions": [
    "bookmarks",
    "topSites",
    "favicon",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "index.html#popup"
  },
  "icons": {
    "48": "small_tiles.png",
    "128": "small_tiles_128.png"
  },
  "manifest_version": 3
}
